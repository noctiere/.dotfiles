{
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "agent": {
    "default_profile": "ask",
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-opus-4.5"
    },
    "favorite_models": [],
    "model_parameters": []
  },
  "auto_install_extensions": {
    "nix": true,
    "git-firefly": true,
    "scss": true,
    "kanagawa-themes": true
  },
  "buffer_font_family": "JetBrainsMono Nerd Font Mono",
  "buffer_font_size": 16,
  "languages": {
    "Nix": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "arguments": [
            "--quiet",
            "--"
          ],
          "command": "alejandra"
        }
      },
      "language_servers": [
        "nil",
        "!nixd"
      ]
    }
  },
  "tabs": {
    "file_icons": true,
    "git_status": true
  },
  "telemetry": {
    "metrics": false
  },
  "theme": "Kanagawa Dragon",
  "ui_font_family": "JetBrainsMono Nerd Font",
  "ui_font_size": 16,
  "vim": {
    "toggle_relative_line_numbers": true,
    "use_smartcase_find": true
  },
  "vim_mode": true
}
